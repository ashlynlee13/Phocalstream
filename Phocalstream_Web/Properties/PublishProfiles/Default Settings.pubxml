          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=.\SQLEXPRESS;Initial Catalog=droughtmonitor;Integrated Security=True;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
