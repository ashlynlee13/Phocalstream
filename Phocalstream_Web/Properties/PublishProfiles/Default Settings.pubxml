          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Phocalstream_Web.Application.Data.ApplicationContext, Phocalstream_Web" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="WaterDBConnection" Order="4" Enabled="False" xmlns="">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=.\SQLEXPRESS;Initial Catalog=WaterData;Integrated Security=True" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\WaterDBConnection_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
